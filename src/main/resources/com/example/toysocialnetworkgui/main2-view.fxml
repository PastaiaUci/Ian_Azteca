<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="mainAnchorPaneView" fx:id="mainAnchorPaneView" prefHeight="578.0" prefWidth="896.0" style="-fx-border-radius: 10px;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.toysocialnetworkgui.Main2Controller">
   <stylesheets>
      <URL value="@main2.css" />
   </stylesheets>
   <children>
      <AnchorPane fx:id="leftAnchorPaneView" layoutX="-11.0" prefHeight="582.0" prefWidth="253.0">
         <children>
            <Pane fx:id="titlePane" layoutX="14.0" prefHeight="90.0" prefWidth="240.0" style="-fx-background-color: #BA2E49 ">
               <children>
                  <Label fx:id="appTitleLabel" layoutX="27.0" layoutY="4.0" prefHeight="36.0" prefWidth="168.0" text="Pixel">
                  </Label>
               </children>
            </Pane>
            <Button fx:id="groupsButton" onAction="#groupsButtonClick" layoutX="29.0" layoutY="148.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="206.0" text="Groups" />
            <Button fx:id="messagesButton" onAction="#onMessagesButtonClick" layoutX="29.0" layoutY="204.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="206.0" text="Messages" />
            <Button fx:id="eventsButton" onAction="#eventsButtonClick" layoutX="29.0" layoutY="259.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="206.0" style="-fx-border-radius: 10px" text="Events" />
            <Button fx:id="editButton" onAction="#editButtonClick" layoutX="31.0" layoutY="375.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="206.0" text="Edit Profile" />
            <Button fx:id="logoutButton"  onAction="#logout"  layoutX="31.0" layoutY="492.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="206.0" text="Logout" />
            <Button fx:id="viewFriendsButton" layoutX="29.0" layoutY="317.0" mnemonicParsing="false" onAction="#onFriendsButtonClick" prefHeight="46.0" prefWidth="206.0" text="Friends" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="profileAnchorPane" layoutX="243.0" layoutY="27.0" prefHeight="128.0" prefWidth="656.0">
         <children>
            <Label fx:id="nameLabel" layoutX="269.0" layoutY="32.0" prefHeight="64.0" prefWidth="290.0" text="Nume Prenume" />
         </children>
      </AnchorPane>
      <ImageView fx:id="profileImage" fitHeight="150.0" fitWidth="178.0" layoutX="283.0" layoutY="83.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Images/cabral.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="friendshipTableView" layoutX="594.0" layoutY="219.0" prefHeight="298.0" prefWidth="284.0">
         <columns>
            <TableColumn fx:id="fromColumn" prefWidth="67.0" text="From" />
            <TableColumn fx:id="toColumn" prefWidth="83.0" text="To" />
            <TableColumn fx:id="statusColumn" prefWidth="75.0" text="Status" />
            <TableColumn fx:id="dateColumn" prefWidth="116.0" text="Data" />
         </columns>
      </TableView>
      <Button fx:id="acceptButton" layoutX="594.0" layoutY="529.0" mnemonicParsing="false" onAction="#acceptRequest" text="Accept" />
      <Button fx:id="rejectButton" layoutX="709.0" layoutY="529.0" mnemonicParsing="false" onAction="#rejectRequest" text="Reject" />
      <Button fx:id="deleteButton" layoutX="824.0" layoutY="529.0" mnemonicParsing="false" text="Delete" />
      <Label fx:id="istoricLabel" layoutX="632.0" layoutY="171.0" prefHeight="40.0" prefWidth="206.0" text="Istoricul cererilor de prietenie" />
   </children>
</AnchorPane>
